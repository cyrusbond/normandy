                "type": {
                  "$id": "#/properties/arguments_schema/properties/properties/properties/preferenceType/properties/type",
                  "type": "string",
                  "title": "The Type Schema",
                  "default": "",
                  "examples": [
                    "string"
                  ],
                  "pattern": "^(.*)$"
                }
              }
            },
