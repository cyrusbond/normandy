{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "count",
    "next",
    "previous",
    "results"
  ],
  "properties": {
    "count": {
      "$id": "#/properties/count",
      "type": "integer",
      "title": "The Count Schema",
      "default": 0,
      "examples": [
        11
      ]
    },
    "next": {
      "$id": "#/properties/next",
      "type": "null",
      "title": "The Next Schema",
      "default": null,
      "examples": [
        null
      ]
    },
    "previous": {
      "$id": "#/properties/previous",
      "type": "null",
      "title": "The Previous Schema",
      "default": null,
      "examples": [
        null
      ]
    },
    "results": {
      "$id": "#/properties/results",
      "type": "array",
      "title": "The Results Schema",
      "items": {
        "$id": "#/properties/results/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "arguments_schema",
          "id",
          "implementation_url",
          "name"
        ],
        "properties": {
          "arguments_schema": {
            "$id": "#/properties/results/items/properties/arguments_schema",
            "type": "object",
            "title": "The Arguments_schema Schema",
            "required": [
              "$schema",
              "properties",
              "required",
              "title",
              "type"
            ],
            "properties": {
              "$schema": {
                "$id": "#/properties/results/items/properties/arguments_schema/properties/$schema",
                "type": "string",
                "title": "The $schema Schema",
                "default": "",
                "examples": [
                  "http://json-schema.org/draft-04/schema#"
                ],
                "pattern": "^(.*)$"
              },
              "properties": {
                "$id": "#/properties/results/items/properties/arguments_schema/properties/properties",
                "type": "object",
                "title": "The Properties Schema",
                "required": [
                  "message"
                ],
                "properties": {
                  "message": {
                    "$id": "#/properties/results/items/properties/arguments_schema/properties/properties/properties/message",
                    "type": "object",
                    "title": "The Message Schema",
                    "required": [
                      "default",
                      "description",
                      "type"
                    ],
                    "properties": {
                      "default": {
                        "$id": "#/properties/results/items/properties/arguments_schema/properties/properties/properties/message/properties/default",
                        "type": "string",
                        "title": "The Default Schema",
                        "default": "",
                        "examples": [
                          ""
                        ],
                        "pattern": "^(.*)$"
                      },
                      "description": {
                        "$id": "#/properties/results/items/properties/arguments_schema/properties/properties/properties/message/properties/description",
                        "type": "string",
                        "title": "The Description Schema",
                        "default": "",
                        "examples": [
                          "Message to log to the console"
                        ],
                        "pattern": "^(.*)$"
                      },
                      "type": {
                        "$id": "#/properties/results/items/properties/arguments_schema/properties/properties/properties/message/properties/type",
                        "type": "string",
                        "title": "The Type Schema",
                        "default": "",
                        "examples": [
                          "string"
                        ],
                        "pattern": "^(.*)$"
                      }
                    }
                  }
                }
              },
              "required": {
                "$id": "#/properties/results/items/properties/arguments_schema/properties/required",
                "type": "array",
                "title": "The Required Schema",
                "items": {
                  "$id": "#/properties/results/items/properties/arguments_schema/properties/required/items",
                  "type": "string",
                  "title": "The Items Schema",
                  "default": "",
                  "examples": [
                    "message"
                  ],
                  "pattern": "^(.*)$"
                }
              },
              "title": {
                "$id": "#/properties/results/items/properties/arguments_schema/properties/title",
                "type": "string",
                "title": "The Title Schema",
                "default": "",
                "examples": [
                  "Log a message to the console"
                ],
                "pattern": "^(.*)$"
              },
              "type": {
                "$id": "#/properties/results/items/properties/arguments_schema/properties/type",
                "type": "string",
                "title": "The Type Schema",
                "default": "",
                "examples": [
                  "object"
                ],
                "pattern": "^(.*)$"
              }
            }
          },
          "id": {
            "$id": "#/properties/results/items/properties/id",
            "type": "integer",
            "title": "The Id Schema",
            "default": 0,
            "examples": [
              1
            ]
          },
          "implementation_url": {
            "$id": "#/properties/results/items/properties/implementation_url",
            "type": "string",
            "title": "The Implementation_url Schema",
            "default": "",
            "examples": [
              "https://normandy.cdn.mozilla.net/api/v1/action/console-log/implementation/sha384-dwDUzk7p67hyOW81gMBIWnhrBFoOvbRvrt2ftxkMrqBjbADVA_1TjHuv0i7bbFrN/"
            ],
            "pattern": "^(.*)$"
          },
          "name": {
            "$id": "#/properties/results/items/properties/name",
            "type": "string",
            "title": "The Name Schema",
            "default": "",
            "examples": [
              "console-log"
            ],
            "pattern": "^(.*)$"
          }
        }
      }
    }
  }
}